{
  "name": "master-only",
  "target": "branch",
  "enforcement": "active",
  "priority": 10,
  "conditions": {
    "ref_name": {
      "include": ["refs/heads/master"],
      "exclude": []
    }
  },
  "rules": [
    {
      "type": "pull_request",
      "parameters": {
        "required_approving_review_count": 0,
        "dismiss_stale_reviews": false,
        "require_code_owner_review": false,
        "require_last_push_approval": false,
        "required_conversation_resolution": false
      }
    },
    {
      "type": "required_status_checks",
      "parameters": {
        "strict_required_status_checks_policy": true,
        "required_checks": [
          { "context": "backend-java CI / unit (pull_request)",      "integration_id": null },
          { "context": "backend-java CI / it (pull_request)",        "integration_id": null },
          { "context": "CI / build (pull_request)",                   "integration_id": null },
          { "context": "backend-java CI / backend-java CI / it-cluster (pull_request)", "integration_id": null }
        ]
      }
    }
  ],
  "bypass_actors": []
}
