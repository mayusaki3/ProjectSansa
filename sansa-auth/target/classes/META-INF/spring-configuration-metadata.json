{
  "groups": [
    {
      "name": "jwt",
      "type": "com.sansa.auth.config.JwtConfig",
      "sourceType": "com.sansa.auth.config.JwtConfig"
    },
    {
      "name": "sansa.cassandra",
      "type": "com.sansa.auth.repo.cassandra.CassandraProps",
      "sourceType": "com.sansa.auth.repo.cassandra.CassandraProps"
    }
  ],
  "properties": [
    {
      "name": "jwt.access-token-minutes",
      "type": "java.lang.Integer",
      "description": "アクセストークンの有効期間（分）",
      "sourceType": "com.sansa.auth.config.JwtConfig",
      "defaultValue": 0
    },
    {
      "name": "jwt.issuer",
      "type": "java.lang.String",
      "description": "iss (Issuer)",
      "sourceType": "com.sansa.auth.config.JwtConfig"
    },
    {
      "name": "jwt.refresh-token-days",
      "type": "java.lang.Integer",
      "description": "リフレッシュトークンの有効期間（日）",
      "sourceType": "com.sansa.auth.config.JwtConfig",
      "defaultValue": 0
    },
    {
      "name": "jwt.secret",
      "type": "java.lang.String",
      "description": "Base64 でエンコードされた共有秘密鍵（HS256\/HS512 用） 例: application-*.yml で jwt.secret: \"xxxxx(base64)xxxxx\"",
      "sourceType": "com.sansa.auth.config.JwtConfig"
    },
    {
      "name": "sansa.cassandra.contact-point",
      "type": "java.lang.String",
      "sourceType": "com.sansa.auth.repo.cassandra.CassandraProps",
      "defaultValue": "127.0.0.1"
    },
    {
      "name": "sansa.cassandra.datacenter",
      "type": "java.lang.String",
      "sourceType": "com.sansa.auth.repo.cassandra.CassandraProps",
      "defaultValue": "datacenter1"
    },
    {
      "name": "sansa.cassandra.keyspace",
      "type": "java.lang.String",
      "sourceType": "com.sansa.auth.repo.cassandra.CassandraProps",
      "defaultValue": "sansa_auth"
    },
    {
      "name": "sansa.cassandra.port",
      "type": "java.lang.Integer",
      "sourceType": "com.sansa.auth.repo.cassandra.CassandraProps",
      "defaultValue": 9042
    }
  ],
  "hints": []
}